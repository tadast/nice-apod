// "content_security_policy": "script-src 'self' https://www.googleapis.com/; object-src 'self'",
{
  "manifest_version": 2,
  "name": "Nice APOD",
  "description": "A nicer looking Astronomy Picture of the Day from NASA",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "images/logo16.png",
    "128": "images/logo128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://apod.nasa.gov/apod/*"],
      "css": ["css/main.css"],
      "js": ["main.js"]
    }
  ],
  "permissions": ["tabs", "http://apod.nasa.gov/apod/*"]
}
